set terminal png size 1200,750 font arial 16
set key right bottom
set yrange [0:105]

set xtics 100
set grid xtics ytics

set xlabel "Training signatures"
set ylabel "%TP"

plot \
	"./results.txt" using 1:2 with linespoints ti "HTTP", \
	"./results.txt" using 1:4 with linespoints ti "IPTV", \
	"./results.txt" using 1:6 with linespoints ti "Skype", \
	"./results.txt" using 1:8 with linespoints ti "DNS", \
	"./results.txt" using 1:10 with linespoints ti "Average"

